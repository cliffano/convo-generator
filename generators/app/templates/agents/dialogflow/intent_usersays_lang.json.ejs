[
  <%
    for (let i = 0, iLen = query.messages[language].length; i < iLen; i++) {
  %>
  {
    "data": [
    <%
      for (let j = 0, jLen = query.messages[language][i].fragments.length; j < jLen; j++) {
    %>
      {
        <%
          if (query.messages[language][i].fragments[j].is_param) {
        %>
        "text": "<%= query.messages[language][i].fragments[j].text %>",
        "alias": "<%= query.messages[language][i].fragments[j].text %>",
        "meta": "@<%= query.messages[language][i].fragments[j].text %>",
        "userDefined": true
        <%
          } else {
        %>
        "text": "<%= query.messages[language][i].fragments[j].text %>",
        "userDefined": false
        <%
          }
        %>
      }<% if (j < jLen - 1) { %>,<% } %>
    <%
      }
    %>
    ],
    "isTemplate": false,
    "count": 0,
    "updated": 0
  }<% if (i < iLen - 1) { %>,<% } %>
  <%
    }
  %>
]
