{
  "name": "Welcome",
  "auto": true,
  "contexts": [],
  "responses": [
    {
      "resetContexts": false,
      "action": "input.welcome",
      "affectedContexts": [],
      "parameters": [],
      "messages": [
      <%
        const languages = Object.keys(config.convoSpec.conversations.greetings.replies);
        for (var i = 0, iLen = languages.length; i < iLen; i++) {
          const replies = config.convoSpec.conversations.greetings.replies[languages[i]];
          for (var j = 0, jLen = replies.length; j < jLen; j++) {
      %>
        {
          "type": "simple_response",
          "lang": "<%= languages[i] %>",
          "platform": "google",
          "textToSpeech": "<%= replies[j] %>"
        }<% if (i < iLen - 1 || j < jLen - 1) { %>,<% } %>
      <%
          }
        }
      %>
      ],
      "defaultResponsePlatforms": {},
      "speech": []
    }
  ],
  "priority": 500000,
  "webhookUsed": false,
  "webhookForSlotFilling": false,
  "fallbackIntent": false,
  "events": [
    {
      "name": "WELCOME"
    },
    {
      "name": "GOOGLE_ASSISTANT_WELCOME"
    }
  ]
}
